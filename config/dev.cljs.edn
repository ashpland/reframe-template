^{:css-dirs ["resources/public/css"]}
{:main sample.core
 :closure-defines {}
 :infer-externs true
 ; :verbose true
 :install-deps false
 :npm-deps false
 :output-dir "target/public/cljs-out/dev"
 :output-to "target/public/cljs-out/dev-main.js"
 :language-out :ecmascript5 ;; see https://dev.clojure.org/jira/browse/CLJS-2376
 :foreign-libs [{:file "target/public/js-out/react.js"
                 :provides ["react"
                            "react-dom"
                            "react-dom-server"
                            "create-react-class"]
                 :global-exports {react React
                                  react-dom ReactDOM
                                  react-dom-server ReactDOMServer
                                  create-react-class createReactClass}}]}
